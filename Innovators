#include <Servo.h>

Servo servo1;
Servo servo2;
Servo servo3;
int buz = 11;
int angle = 0;

void setup() {
  servo1.attach(6);
  servo1.write(angle);
    servo2.attach(10);
  servo2.write(angle);
    servo3.attach(9);
  servo3.write(angle);
  pinMode(buz,OUTPUT);

}


void loop() 
{

    digitalWrite(buz,HIGH);
delay(500);

digitalWrite(buz,LOW);
delay(300);

